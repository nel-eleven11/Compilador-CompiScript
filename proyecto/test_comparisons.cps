// test_comparisons.cps
// Test comparison operations

let a: integer = 10;
let b: integer = 5;

// Test all comparisons
let lt: boolean = a < b;    // false (10 < 5)
let gt: boolean = a > b;    // true (10 > 5)
let eq: boolean = a == b;   // false (10 == 5)
let neq: boolean = a != b;  // true (10 != 5)
let lte: boolean = a <= b;  // false (10 <= 5)
let gte: boolean = a >= b;  // true (10 >= 5)
